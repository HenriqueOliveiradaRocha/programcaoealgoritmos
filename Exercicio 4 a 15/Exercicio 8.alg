algoritmo "semnome"
// Função :
// Autor :
// Data : 22/08/2018
// Seção de Declarações 
var
an,aa,ida,ida50 : inteiro
inicio
// Seção de Comandos 
escreval (" Calcule sua idade")
escreval (" Digite o ano do seu nascimento")
leia (an)
escreval ("digite o ano atual")
leia (aa)
ida <- aa-an
ida50 <- 2050-an
escreval (" Sua idade atual é",ida, "em 2050 sua idade será",ida50)
fimalgoritmo
