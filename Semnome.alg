algoritmo "calculadora básica"
// Função :
// Autor : Henrique Oliveira
// Data : 22/08/2018
// Seção de Declarações 
var N1,N2,res : inteiro

inicio
// Seção de Comandos
escreval ("soma de dois numeros inteiros")
escreval ("digite o primeiro valor")
leia (N1)
escreval ("digite o segundo valor")
leia (N2)
res <- N1+N2
escreval ("a soma entre",N1,"e",N2,"é",res)
fimalgoritmo
